{
   "plugins": [
       "react-hot-loader/babel",
       [
           "functional-composition", {
               "name": "pipe",
               "as": "pipe",
               "from": "ramda",
               }
        ]
    ],
   "presets": ["es2015", "react", "react-hmre", "stage-0"],
   "env": {
    "development": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-catch-errors",
          "imports": [
            "react",
            "redbox-react"
          ]
        }]
      }]]
    }
  }
}

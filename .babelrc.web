{
   "plugins": [
      [
         "functional-composition", {
               "name": "pipe",
               "as": "pipe",
               "from": "ramda"
         }
      ],
      "react-hot-loader/babel",
      [
         "react-transform", {
           "transforms": [{
               "transform": "react-transform-catch-errors",
               "imports": [
                  "react",
                  "redbox-react"
               ]
            }]
         }
      ],
      "syntax-flow",
      "transform-flow-strip-types",
      "typecheck"
   ],
   "presets": ["es2015", "react", "react-hmre", "stage-0"]
}
